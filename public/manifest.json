{
    "manifest_version": 3,
    "name": "Vector History Search",
    "description": "This extension embeds your browsing history.",
    "version": "1.0",
    "icons": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "action": {
      "default_popup": "index.html"
    },
    "permissions": [
      "activeTab",
      "scripting",
      "storage",
      "alarms"
   ],
   "host_permissions": ["https://*/*", "http://*/*"],
   "background": {
      "service_worker": "background.js",
      "type": "module"
   },
   "content_security_policy": {
      "extension_pages": "script-src 'self' 'wasm-unsafe-eval'"
   }
  }
